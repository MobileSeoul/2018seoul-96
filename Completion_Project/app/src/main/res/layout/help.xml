            android:layout_weight="1"
            android:layout_column="4"
            android:includeFontPadding="false"
            android:background="@color/color1"
            android:gravity="center"

            android:text="0.15~"
            android:textColor="#424242"
            android:textSize="13sp" />

    </LinearLayout>

    <LinearLayout
