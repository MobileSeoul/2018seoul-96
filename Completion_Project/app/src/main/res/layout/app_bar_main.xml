<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >





    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/transparent"
        android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen">

        <android.support.design.widget.CollapsingToolbarLayout
            android:id="@+id/collapsingToolbarLayout01"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fitsSystemWindows="true"
            app:contentScrim="@android:color/white"
            app:expandedTitleMarginStart="48dp"
            app:layout_scrollFlags="scroll|exitUntilCollapsed">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >

                <LinearLayout
                    android:id="@+id/gon1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"

                    android:weightSum="18">
                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="13">

                        <ImageView
                            android:id="@+id/imageView01"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fitsSystemWindows="true"
                            android:scaleType="fitXY"
                            app:layout_collapseMode="parallax" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:scaleType="fitXY"
                            android:fitsSystemWindows="true"
                            android:id="@+id/back"></ImageView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:id="@+id/gon2"
                            >
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:layout_height="match_parent"></LinearLayout>
                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="horizontal"
                                   >

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:orientation="horizontal"
                                        android:weightSum="8">

                                        <android.support.percent.PercentFrameLayout
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="2">

                                            <TextView
                                                android:layout_width="1dp"
                                                android:layout_height="match_parent"
                                                android:layout_gravity="center|right"
                                                android:background="#777777"
                                                app:layout_heightPercent="33%" />
                                        </android.support.percent.PercentFrameLayout>

                                        <android.support.percent.PercentFrameLayout xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="5">

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="match_parent"
                                                android:gravity="center"
                                                android:orientation="vertical"
                                                app:layout_widthPercent="100%">

                                                <TextView
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="2" />

                                                <TextView
                                                    android:id="@+id/status"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="1"
                                                    android:gravity="center"
                                                    android:includeFontPadding="false"
                                                    android:text="상태유무"
                                                    android:textColor="#424242"
                                                    android:textSize="30dp" />

                                                <TextView
                                                    android:id="@+id/status2"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="1"
                                                    android:gravity="center"
                                                    android:includeFontPadding="false"
                                                    android:text="부가사항"
                                                    android:textColor="#424242"
                                                    android:textSize="16dp" />

                                                <TextView
                                                    android:layout_width="match_parent"
                                                    android:layout_height="0dp"
                                                    android:layout_weight="2" />
                                            </LinearLayout>
                                        </android.support.percent.PercentFrameLayout>
                                        <TextView
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_weight="1"
                                            />
                                    </LinearLayout>
                                </LinearLayout>
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    android:layout_weight="1"
                                    android:id="@+id/status3"
                                    android:gravity="center"
                                    android:textSize="20sp"
                                    android:textColor="#000000"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                    </FrameLayout>


                    <TextView
                        android:id="@+id/editText1"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:ems="10"
                        android:gravity="center"
                        android:inputType="textPersonName"
                        android:text="현재"
                        android:textColor="#33b5e5"
                        android:background="#efefef" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="#efefef">

                            <andbook.ecample.mun.Text
                                android:includeFontPadding="false"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:ems="10"
                                android:layout_weight="1"
                                android:textSize="10sp"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:text="CAI\n통합대기등급" />
                            <TextView
                                android:id="@+id/fir"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#FF6161"
                                android:text="통합대기등급" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="#efefef">

                            <andbook.ecample.mun.Text
                                android:includeFontPadding="false"

                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textSize="10sp"
                                android:textColor="#000000"
                                android:text="NO2\n이산화질소" />
                            <TextView
                                android:id="@+id/sec"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#FF6161"
                                android:text="이산화질소" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="#efefef">

                            <andbook.ecample.mun.Text
                                android:includeFontPadding="false"

                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="10sp"
                                android:text="SO2\n아황산가스" />
                            <TextView
                                android:id="@+id/thir"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#FF6161"
                                android:text="등급" />
                        </LinearLayout>
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:paddingLeft="@dimen/activity_horizontal_margin2"
                        android:paddingRight="@dimen/activity_horizontal_margin2"
                        android:background="#efefef">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_gravity="center"
                            android:background="#777777" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="#efefef">

                            <andbook.ecample.mun.Text
                                android:includeFontPadding="false"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="10sp"
                                android:text="PM2.5\n초미세먼지" />
                            <TextView
                                android:id="@+id/fiv"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#FF6161"

                                android:text="통합대기수치" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="#efefef">

                            <andbook.ecample.mun.Text
                                android:includeFontPadding="false"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="10sp"
                                android:text="CO\n일산화탄소" />
                            <TextView
                                android:id="@+id/six"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#FF6161"
                                android:text="미세먼지 수치" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="#efefef">

                            <andbook.ecample.mun.Text
                                android:includeFontPadding="false"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:textSize="10sp"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:text="O3\n오존" />
                            <TextView
                                android:id="@+id/sev"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:ems="10"
                                android:gravity="center"
                                android:textColor="#FF6161"
                                android:text="초미세먼지 수치" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:background="#efefef">

                        <Button
                            android:id="@+id/buttonex"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:ems="10"
                            android:gravity="center"
                            android:background="@android:color/transparent"
                            android:inputType="textPersonName"
                            android:text="상세정보"
                            android:textColor="#33b5e5" />
                    </LinearLayout>

                </LinearLayout>


            </FrameLayout>

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                app:layout_collapseMode="pin"
                android:background="@android:color/transparent"
                app:layout_scrollFlags="scroll|enterAlways"
                app:popupTheme="@style/ThemeOverlay.AppCompat.Light"

                app:title="@string/app_name">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:paddingLeft="20dp">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="5"
                        android:orientation="vertical">
                        <andbook.ecample.mun.Text
                            android:includeFontPadding="false"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center|bottom"
                            android:textSize="13sp"
                            android:text="먼지가먼지"
                            android:textColor="#424242"
                            android:id="@+id/toolbartext"
                            />
                    <andbook.ecample.mun.Text
                        android:includeFontPadding="false"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:textSize="12sp"
                        android:text="먼지가먼지"
                        android:textColor="#FF6161"
                        android:id="@+id/toolbartext2"
                        />

                    </LinearLayout>

                    <ImageButton
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/loc"
                        android:tint="#33b5e5"
                        android:src="@drawable/ic_action_click"
                        android:background="@android:color/transparent"
                        android:paddingLeft="20dp"
                        />
                </LinearLayout>

            </android.support.v7.widget.Toolbar>
        </android.support.design.widget.CollapsingToolbarLayout>
    </android.support.design.widget.AppBarLayout>

    <include layout="@layout/content_main" />

</android.support.design.widget.CoordinatorLayout>
